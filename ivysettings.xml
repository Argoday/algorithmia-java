<?xml version="1.0"?>
<ivysettings>

  <settings defaultResolver="maven-central" defaultConflictManager="all"/>

  <resolvers>

    <!-- Algorithmia local repo (for publishing to) -->
    <filesystem name="algorithmia-local" m2compatible="true">
      <!-- new style: <artifact pattern="/algorithmia/maven/releases/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/> -->
      <ivy pattern="/algorithmia/oldmaven/[organisation]/[module]/ivy.xml"/>
      <artifact pattern="/algorithmia/oldmaven/[organisation]/[module]/[artifact]-[revision].[ext]"/>
      <!-- Default pattern for SBT: -->
      <!-- <artifact pattern="/git/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/> -->
    </filesystem>

    <!-- Maven Central repo -->
    <ibiblio name="maven-central" m2compatible="true"/>

  </resolvers>

</ivysettings>
